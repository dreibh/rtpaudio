#ifndef PINGERHOST_ICC
#define PINGERHOST_ICC


#include "pingerhost.h"


// ###### Operator "==" #####################################################
inline int operator==(const PingerHost& ph1, const PingerHost& ph2)
{
   return((ph1.Address      == ph2.Address) &&
          (ph1.TrafficClass == ph2.TrafficClass));
}


// ###### Operator "!=" #####################################################
inline int operator!=(const PingerHost& ph1, const PingerHost& ph2)
{
   return((ph1.Address      != ph2.Address) ||
          (ph1.TrafficClass != ph2.TrafficClass));
}


// ###### Operator "<" ######################################################
inline int operator<(const PingerHost& ph1, const PingerHost& ph2)
{
   if(ph1.Address < ph2.Address)
      return(true);
   else
      return(ph1.TrafficClass < ph2.TrafficClass);
}


// ###### Operator ">" ######################################################
inline int operator>(const PingerHost& ph1, const PingerHost& ph2)
{
   if(ph1.Address > ph2.Address)
      return(true);
   else
      return(ph1.TrafficClass > ph2.TrafficClass);
}


#endif
